{
  "name": "@gengarden/generators",
  "version": "0.0.0",
  "description": "Procedurally generated gardens",
  "main": "build/lib/index.js",
  "files": [
    "build/lib/**/*"
  ],
  "author": "Dean Pisani <dean.a.pisani@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "mocha",
    "generate-examples": "STORY_TARGET_DIR=build/stories glob-run node -r source-map-support/register build/lib/**/examples.js",
    "generate-examples:watch": "nodemon --delay 2 --watch build/lib --exec yarn generate-examples"
  },
  "devDependencies": {
    "@gengarden/examples": "0.0.0",
    "@types/bezier-js": "^0.0.8",
    "@types/gaussian": "^1.1.1",
    "@types/gl-matrix": "^2.4.4",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.9.2",
    "@types/seedrandom": "^2.4.27",
    "@types/should": "^13.0.0",
    "@types/sinon": "^9.0.4",
    "glob-run": "^0.1.7",
    "mocha": "^7.0.1",
    "nodemon": "^1.19.1",
    "should": "^13.2.3",
    "sinon": "^9.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bezier-js": "^2.5.1",
    "canvas": "^2.6.1",
    "gaussian": "^1.1.0",
    "gl-matrix": "^2.8.1",
    "gltf-builder": "^0.7.1",
    "lodash": "^4.17.21",
    "seedrandom": "^2.4.4",
    "tess2": "^1.0.0"
  }
}
